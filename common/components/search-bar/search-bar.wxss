.search-container{
  width: 600rpx;
  padding: 15rpx 20rpx;
  background-color: rgba(246, 246, 246, 1);
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.text-one-row{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  color: #ccc;
}