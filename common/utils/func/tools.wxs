var isEmpty = function(data){
  console.log()
  if(!data || data === undefined || !data==='' || data===0 || data===' ' || data===null)return true
  if (data.constructor =='Array' && data.length==0)return true
  if (data.constructor == 'Object' && JSON.stringify(data)=='{}')return true
  return false
}
module.exports = {
  isEmpty: isEmpty
}