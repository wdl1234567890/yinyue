<wrapper nav-bar-class="theme-module-color" bindgetcontentheight="getContentHeight">
  <view slot="nav-bar-left" class="back-icon">
    <i-icon type="return" size="25" color="#888" />
  </view>
  <view slot="nav-bar-middle">
    <search-bar />
  </view>
  <!--滚动内容区-->
  <view slot="content" class="theme-module-color">
    <search-tab-card tabTitles="{{tabTitles}}" contentHeight="{{contentHeight}}" />
  </view>
</wrapper>