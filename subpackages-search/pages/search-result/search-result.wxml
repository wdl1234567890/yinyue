<wxs src="../../../common/utils/func/tools.wxs" module="tools"/>
<wrapper nav-bar-class="theme-module-color" bindgetcontentheight="getContentHeight" has-music-list="{{hasMusicList}}" bindmusicplayitemchange="musicPlayItemChangeInner" music-info="{{musicInfo}}">
  <view slot="nav-bar-left" class="back-icon">
    <back-button />
  </view>
  <view slot="nav-bar-middle">
    <search-bar catchsearch="search" search-value="{{searchValue}}"/>
  </view>
  <!--滚动内容区-->
  <view slot="content" class="theme-module-color">
    <search-tab-card tabTitles="{{tabTitles}}" contentHeight="{{contentHeight}}" sing-datas="{{singDatas}}" sing-list-datas="{{singListDatas}}" catchmusicplayitemchange="musicPlayItemChangeInner"/>
  </view>
</wrapper>
